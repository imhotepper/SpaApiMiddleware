<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>

    <PackageId>SpaApiMiddleware</PackageId>
    <Version>1.0.2</Version>
    <Authors>dragos stefanescu</Authors>    
    <projectUrl>https://github.com/imhotepper/SpaApiMiddleware</projectUrl>
    <description>Enable SPA calls to be served from wwwwroot and only /api calls to go through dotnet core controllers.</description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.1.2" />
  </ItemGroup>
  
</Project>
